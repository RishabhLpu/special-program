//function that takes something return something.
#include <iostream>
using namespace std ; 
int sum(int,int);
int main() {
    // Write C++ code here
    int a=5; //a and b are actual arguments
    int b=8;
    int s=sum(a,b);
    cout<<"sum is ="<<s;
}
int sum(int x, int y)// formal arguments.
{
    return(x+y);
}
